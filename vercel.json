{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "index.mjs": {
      "runtime": "nodejs18.x"
    },
    "api/*.mjs": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    { "src": "/api/healthcheck", "dest": "/api/healthcheck.mjs" },
    { "src": "/api/(.*)", "dest": "/api/index.mjs" },
    { "src": "/(.*)", "dest": "/index.mjs" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}