{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "functions": {
    "api/*.js": {
      "runtime": "vercel-node@2.0.0",
      "memory": 1024
    }
  },
  "routes": [
    { "src": "/api/healthcheck", "dest": "/api/healthcheck.js" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "env": {
    "VERCEL": "1",
    "NODE_OPTIONS": "--enable-source-maps"
  }
}